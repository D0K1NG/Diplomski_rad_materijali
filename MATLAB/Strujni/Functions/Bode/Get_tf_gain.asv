function gain = Get_tf_gain(tf)

% Calculate gain:
[num, den] = tfdata(tf, 'v')

% Get poles at origin:
np_int = 0;
for k = length(den):-1:1
    if den(k) == 0
        np_int = np_int + 1;
    else
        break
    end
end

% Get zeros at origin:
nz_int = 0;
for k = length(den):-1:1
    if den(k) == 0
        nz_int = nz_int + 1;
    else
        break
    end
end

% Remove 
den_no_int = den(1:end-np_int);
K = num(end)/den_no_int(end);


end

